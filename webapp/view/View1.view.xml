<mvc:View controllerName="graph.ChartGraph.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
xmlns:viz ="sap.viz.ui5.controls" 
xmlns:data="sap.viz.ui5.data" 
xmlns:feeds="sap.viz.ui5.controls.common.feeds">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="GRAPH">
					<content>
						<viz:VizFrame vizType="column" width="100%" height="100%" vizProperties="{ title: {text : 'Assessment Report', visible : true}}">
							<viz:dataset>
								<data:FlattenedDataset data="{/SAP}">
									<data:dimensions>
										<data:DimensionDefinition name="Module" value="{Module}"></data:DimensionDefinition>
									</data:dimensions>
									<data:measures>
										<data:MeasureDefinition name="Marks" value="{Marks}"></data:MeasureDefinition>
									</data:measures>
								</data:FlattenedDataset>
								//test1
							</viz:dataset>
							<viz:feeds>
								<feeds:FeedItem type="Dimension" values="Module" uid="categoryAxis"></feeds:FeedItem>
								<feeds:FeedItem type="Measure" values="Marks" uid="valueAxis"></feeds:FeedItem>
							</viz:feeds>
						</viz:VizFrame>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>